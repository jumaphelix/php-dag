{
  "name": "jumaphelix/php-dag",
  "description": "Implementation of Directed Acyclic Graphs (DAG) based task executions with parallelization in PHP",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-swoole": "*",
    "graphp/graph": "^0.9.3",
    "graphp/algorithms": "^0.8.2",
    "graphp/graphviz": "^0.2.2"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "autoload": {
    "psr-4": {
      "JumaPhelix\\DAG\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JumaPhelix\\DAG\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3"
  },
  "authors": [
    {
      "name": "Phelix Juma",
      "email": "jumaphelix@gmail.com"
    }
  ]
}
